<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net48</TargetFramework>
	<LangVersion>8.0</LangVersion>
    <RootNamespace>Forest_Parser</RootNamespace>
    <ImplicitUsings>disable</ImplicitUsings>
